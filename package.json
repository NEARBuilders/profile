    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3"
  },
  "devDependencies": {
    "@module-federation/rsbuild-plugin": "^0.8.7",
    "@rsbuild/core": "^1.2.3",
    "@rsbuild/plugin-node-polyfill": "^1.3.0",
