{
  "name": "@efiz.near/profile",
  "version": "0.0.1",
  "homepage": "/",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cat welcome.txt && rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
